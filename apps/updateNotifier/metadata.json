{
    "id": "updateNotifier",
    "name": "Update Notefier",
    "shortName": "Update Noti",
    "version": "0.01",
    "description": "Automatecely checks for App updates an notefies you.\n!Important: it requires the new Bangle.js Gadgetbridge App and internet acces enabled!",
    "icon": "app.png",
    "type": "app",
    "tags": "tool,messages,notifications,gadgetbridge,internet",
    "supports": [
        "BANGLEJS2"
    ],
    "dependencies": {
        "scheduler": "type",
        "android": "app"
    },
    "readme": "README.md",
    "storage": [
        {
            "name": "updateNotifier.settings.js",
            "url": "settings.js"
        },
        {
            "name": "updateNotifier.app.js",
            "url": "app.js"
        },
        {
            "name": "updateNotifier.img",
            "url": "app-icon.js",
            "evaluate": true
        }
    ]
}
